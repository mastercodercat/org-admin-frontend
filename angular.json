            },
            "development": {
              "browserTarget": "forms:build:development",
              "port": 4203
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "forms:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/forms/src/test.ts",
            "polyfills": "projects/forms/src/polyfills.ts",
            "tsConfig": "projects/forms/tsconfig.spec.json",
            "karmaConfig": "projects/forms/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/forms/src/favicon.ico",
              "projects/forms/src/assets"
            ],
            "styles": [
              "projects/forms/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "org-comp-lib": {
      "projectType": "library",
      "root": "projects/org-comp-lib",
      "sourceRoot": "projects/org-comp-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/org-comp-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/org-comp-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/org-comp-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/org-comp-lib/src/test.ts",
            "tsConfig": "projects/org-comp-lib/tsconfig.spec.json",
            "karmaConfig": "projects/org-comp-lib/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "organizer-frontends"
}
