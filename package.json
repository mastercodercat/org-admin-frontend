{
  "name": "organizer-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href=/${BITBUCKET_COMMIT}/",
    "build-sandbox": "npm run build -- --configuration=sandbox",
    "build-staging": "npm run build -- --configuration=staging",
    "build-production": "npm run build -- --production",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "npm run test -- --watch=false --browsers=ChromeHeadless",
    "generate": "graphql-codegen --config src/app/shared/services/graphql/codegen/codegen.yml"
  },
  "private": true,
