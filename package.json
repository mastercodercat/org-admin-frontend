    "@angular-eslint/eslint-plugin-template": "~12.2.2",
    "@angular-eslint/schematics": "~12.2.2",
    "@angular-eslint/template-parser": "~12.2.2",
    "@angular/cli": "^12.2.7",
    "@angular/compiler-cli": "^12.2.7",
    "@angularclass/hmr": "^3.0.0",
    "@babel/core": "^7.16.12",
    "@compodoc/compodoc": "^1.1.18",
