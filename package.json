{
  "name": "organizer-frontends",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-hashing all",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-sandbox": "npm run build -- --configuration=sandbox",
    "build-staging": "npm run build -- --configuration=staging",
    "build-production": "npm run build -- --production",
    "build-library": "ng build org-comp-lib",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "hmr": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --configuration hmr",
    "test:ci": "npm run test -- --watch=false --browsers=ChromeHeadless",
    "generate-admin": "graphql-codegen --config projects/admin/src/app/shared/services/graphql/codegen/codegen.yml",
    "generate-explorer": "graphql-codegen --config codegen.yml",
    "generate-toolkit": "graphql-codegen --config projects/toolkit/src/graphql/codegen/codegen.yml",
    "admin": "ng serve --project=admin",
    "explorer": "ng serve --project=explorer",
    "toolkit": "ng serve --project=toolkit",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^12.2.5",
    "@angular/animations": "^12.2.7",
    "@angular/common": "^12.2.7",
    "@angular/compiler": "^12.2.7",
    "@angular/core": "^12.2.7",
    "@angular/forms": "^12.2.7",
    "@angular/platform-browser": "^12.2.7",
    "@angular/platform-browser-dynamic": "^12.2.7",
    "@angular/router": "^12.2.7",
    "@apollo/client": "^3.4.13",
    "@auth0/angular-jwt": "^5.0.2",
    "@auth0/auth0-angular": "^1.7.0",
    "@highcharts/map-collection": "^1.1.3",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/entity": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "@ngrx/store-devtools": "^12.4.0",
    "@schematics/angular": "^12.2.7",
    "@types/auth0-js": "^9.14.5",
    "@types/mixpanel-browser": "^2.35.7",
    "@types/pako": "^1.0.2",
    "apollo-angular": "^2.6.0",
    "auth0-js": "^9.16.4",
    "bootstrap": "^5.1.0",
    "graphql": "^15.6.0",
    "highcharts": "^9.2.2",
